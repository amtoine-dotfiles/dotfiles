super + Escape
    pkill -USR1 -x sxhkd

XF86MonBrightness{Down,Up}
    monitor-manager.nu set-brightness {down,up} --notify
XF86Audio{Lower,Raise}Volume
    sound-manager.nu set-volume {down,up} --notify
XF86AudioMute
    sound-manager.nu toggle-mute --notify

super + Return
    st -e nu --commands '$env.SHELL = $nu.current-exe; tmux new-session -A -s (random uuid)'
super + {_,shift,alt} + @space
    rofi -show {drun,run,window}

super + shift + q
    logout.nu --lock slock --launcher rofi --no-ansi
super + p
    passme.nu -l 10 -bw 5 -p "Select a password to copy"

super + z; t
    notify-date.nu
super + z; b
    notify-battery.nu --report --min 15 --max 85
